<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<h1>New Artist</h1>

<%= form_for @artist do |f|%>
<%=f.label :name%>
<%=f.text_field :name%>
<%=f.label :title%>
<%=f.text_field :title%>
<%=f.label :age%>
<%=f.number_field :age%>
<%=f.submit%>
<%end%>